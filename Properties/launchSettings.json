{
  "profiles": {
    "ApacheKafkaProducerDemo": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/producer",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7125;http://localhost:5125"
